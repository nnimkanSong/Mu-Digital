generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model horoscope_positions {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  category    String
  description String
  name        String
  sequence    Int
  targetDegree Int
}
